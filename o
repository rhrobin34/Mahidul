#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int T;
    cin >> T;

    for (int t = 1; t <= T; t++) {
        int n, q;
        cin >> n >> q;

        vector<int> p(n);
        for (int i = 0; i < n; i++) {
            cin >> p[i];
        }

        cout << "Case " << t << ":\n";

        for (int i = 0; i < q; i++) {
            int A, B;
            cin >> A >> B;

            int l = lower_bound(p.begin(), p.end(), A) - p.begin();
            int r = upper_bound(p.begin(), p.end(), B) - p.begin();

            cout << (r - l) << "\n";
        }
    }
}

